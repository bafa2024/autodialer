# Application Environment
APP_ENV=development
APP_NAME="AutoDial Pro"
APP_VERSION=2.0.0
APP_URL=http://localhost
APP_DEBUG=true

# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_DATABASE=autodialer_dev
DB_USERNAME=autodialer_user
DB_PASSWORD=your_secure_password
DB_CHARSET=utf8mb4

# Security Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRATION=3600
REFRESH_TOKEN_EXPIRATION=604800
PASSWORD_MIN_LENGTH=8
PASSWORD_REQUIRE_SPECIAL=true
MAX_LOGIN_ATTEMPTS=5
LOCKOUT_DURATION=900
SESSION_TIMEOUT=1800
CSRF_TOKEN_EXPIRATION=3600

# Rate Limiting
RATE_LIMIT_API=100
RATE_LIMIT_AUTH=5
RATE_LIMIT_DIALING=1000

# Real-time Features
WEBSOCKET_PORT=8080
PUSHER_APP_ID=your-pusher-app-id
PUSHER_APP_KEY=your-pusher-key
PUSHER_APP_SECRET=your-pusher-secret
PUSHER_CLUSTER=us2

# VoIP Integration
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890
VONAGE_API_KEY=your-vonage-api-key
VONAGE_API_SECRET=your-vonage-api-secret

# Email Configuration
MAIL_DRIVER=smtp
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_ENCRYPTION=tls
MAIL_USERNAME=noreply@autodialpro.com
MAIL_PASSWORD=your-email-password
MAIL_FROM_ADDRESS=noreply@autodialpro.com
MAIL_FROM_NAME="AutoDial Pro"

# AI Services
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=1000
AI_SENTIMENT_ANALYSIS=true
AI_CALL_SUMMARIZATION=true
AI_VOICE_RECOGNITION=true

# File Storage
STORAGE_DRIVER=local
STORAGE_PATH=storage/
MAX_FILE_SIZE=10485760
ALLOWED_EXTENSIONS=mp3,wav,csv,xlsx,xls,pdf

# Features
FEATURE_AI_AGENTS=true
FEATURE_CALL_RECORDING=true
FEATURE_VOICEMAIL_DETECTION=true
FEATURE_CRM_INTEGRATION=true
FEATURE_EMAIL_CAMPAIGNS=true
FEATURE_REAL_TIME_ANALYTICS=true
FEATURE_MULTI_TENANT=false

# Logging
LOG_LEVEL=debug
LOG_CHANNEL=stack

# Timezone
TIMEZONE=UTC
LOCALE=en 