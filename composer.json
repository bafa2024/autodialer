{
    "name": "autodialer/application",
    "description": "AutoDial Pro - Enterprise Auto Dialer Solution",
    "type": "project",
    "require": {
        "php": ">=8.0.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-openssl": "*",
        "ext-curl": "*",
        "firebase/php-jwt": "^6.0",
        "vlucas/phpdotenv": "^5.0",
        "monolog/monolog": "^3.0",
        "phpmailer/phpmailer": "^6.0",
        "guzzlehttp/guzzle": "^7.0",
        "ratchet/pawl": "^0.4.0",
        "cboden/ratchet": "^0.4.0",
        "pusher/pusher-php-server": "^7.0",
        "ramsey/uuid": "^4.0",
        "paragonie/sodium_compat": "^1.0",
        "paragonie/random_compat": "^2.0",
        "league/oauth2-client": "^2.0",
        "twilio/sdk": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/",
            "Config\\": "config/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
